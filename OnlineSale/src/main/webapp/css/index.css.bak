*{margin:0;padding:0;font-family:"Microsoft yahei";}
a{text-decoration:none;color:#fff;}
img{border:0;}
li{list-style:none;}
.fl{float:left;}
.fr{float:right;}
.clear{clear:both;}
.re{color:#F46060;}
#main{width:1060px;margin:0 auto;}
#main .top{width:100%;height:30px;line-height:30px;font-size:14px;color:#fff;background-color:#F46060;}
#main .top .left{padding:0 80px;width:160px;height:30px;}
#main .top .left a.log{margin-left:30px;margin-right:5px;}
#main .top .right{width:450px;height:30px;padding-left:10px;}
#main .top .right a{margin-right:12px;}
#main .top .right a img{vertical-align:middle;}
#main .other{width:100%;height:30px;background-color:#C4C4C4;}

#main .second{margin-left:7%;height:101px;width:93%;}
#main .second > span{font-size:24px;display:inline-block;width:140px;height:101px;text-align:center;line-height:101px;}
#main .second .search{display:inline-block;height:101px;line-height:101px;}
#main .second .search input{border-width:2px 0 2px 2px;border-color:#F46060;border-style:solid;
height:24px;line-height:24px;padding:4px;outline:0;font-size:14px;}
#main .second .search .button{width:82px;height:36px;background:#F46060;border:none;line-height:1;
color:#fff;font-size:16px;cursor:pointer;margin-left:-7px;}
/*订单详情页*/
#main .second .address{width:70%;margin:25px auto;}
#main .second .address span{background:#eee;display:inline-block;width:19%;text-align:center;height:51px;line-height:51px;}
#main .second .address span.hover{background:#F46060;color:#fff;}
/*地址详情管理*/
#main .addressChoose{width:86%;height:40px;line-height:40px;margin:10px 0;border-bottom:1px solid #cbcbcb;
padding:0 7%;}
#main .addressManage{width:100%;margin:30px auto 24px;}
#main .addressManage .item{margin:10px 80px;padding:10px 0;}
#main .addressManage .item .addressModify{width:10%;display:inline-block;text-align:center;cursor:pointer;}

#main .addressManage .item span strong{margin:0 20px;}
#main .addressManage .item .addressDefault{margin-right:20px;display:inline-block;}
#main .addressManage .addressMore{margin-left:100px;border:1px solid #cbcbcb;width:100px;height:40px;line-height:40px;
text-align:center;cursor:pointer;}


#main .cartMain{width:100%;}
#main .cartMain .cartThead,#main .cartMain .cartItem{width:100%;border:1px solid #e9e9e9;}
#main .cartMain .cartThead{height:32px;line-height:32px;background:#f3f3f3;padding:5px 0;margin:0 0 10px;}
#main .cartMain .cartItem{height:120px;line-height:80px;margin-bottom:10px;}
#main .cartMain .cartCheck{padding-left:2%;}
.cartGoods{width:32%;}
.cartWidth{width:11%;}
.cartWidthA{width:20%;text-align:center;height:32px;}
.cartWidthB{width:20%;text-align:center;height:120px;}
#main .cartMain .cartProps{height:32px;}
#main .cartMain .cartGoods img{width:100px;height:100px;vertical-align:middle;margin-left:-50px;margin-top:10px;
margin-right:10px;}
#main .cartMain .cartGoods span{color:#000;}
#main .cartMain .cartGoods span:hover{color:#F46060;}
#main .cartMain .props-txt{white-space:nowrap;text-overflow:ellipsis;overflow:hidden;}
#main .cartMain .reduce,#main .cartMain .add{height:18px;color:#666;border:1px solid #cacbcb;
padding:1px 0;text-align:center;width:16px;display:inline-block;line-height:18px;}
#main .cartMain .reduce{border-right:0;}
#main .cartMain .add{border-left:0;}
#main .cartMain .numInput{margin:-4%;text-align:center;width:42px;height:18px;padding:1px;
border:1px solid #cacbcb;display:inline-block;}
#main .cartMain .cartHandle{padding-top:18px;}
#main .cartMain .cartHandle a{color:#000;display:block;line-height:40px;}
#main .cartMain .cartHandle a:hover{color:#F46060;}
.check{margin-right:10px;}



/*固定在底部的结算开始*/
.cartFootbar{background:#fff;position:fixed;bottom:0;z-index:100;height:50px;line-height:50px;
width:1423px;border:1px solid #f0f0f0;margin:0 auto;}
.cartFootbar a{color:#000;}
.cartFootbar a:hover{color:#F46060;}
.cartFootbar a.finish{display:inline-block;width:100px;height:50px;line-height:50px;background:#e4393c;
color:#fff;text-align:center;border-radius:10px;}
.cartFootbarWrap{width:1060px;margin:0 auto;}
.cartFootbar span{margin-right:10px;}
.cartFootbar strong{margin:0 5px;color:#F46060;}

/*备注样式开始*/
#main .note{width:84%;margin:4% 8%;}
#main .note input{width:60%;}
#main  .cartPay{margin-right:8%;margin-top:3%;}
#main  .cartPay a{display:inline-block;padding:10px;margin:0 10px;background:#F46060;border-radius:16px;cursor:pointer;}

.service{width:1060px;margin:80px auto 30px;background:#c4c4c4;height:54px;line-height:54px;font-size:17px;}
.service span{width:24%;display:inline-block;text-align:center;}
.serviceFooter{width:1060px;margin:0 auto;margin-bottom:120px;}
.serviceFooter a{color:#666;}
.serviceFooter dl{float:left;width:25%;text-align:center;color:#666;}
.serviceFooter dl dt{padding:6px 0;font:400 16px/24px "microsoft yahei";}



